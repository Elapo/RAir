<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">

    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-outcome>dashAirports</from-outcome>
            <to-view-id>/admin/airports/airports.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>airportDetails</from-outcome>
            <to-view-id>/admin/airports/airportDetails.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addAirport</from-outcome>
            <to-view-id>/admin/airports/addAirport.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>